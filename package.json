{
  "name": "sb-career-portal",
  "private": true,
  "version": "1.0.1",
  "type": "module",
  "scripts": {
    "dev": "vite --host",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "format": "prettier --write src/**/*.ts{,x}",
    "lint": "tsc --noEmit && eslint src/**/*.ts{,x}",
    "deploy-firebase": "npm run build && firebase deploy",
    "codegen": "openapi-generator-cli generate -i openapi/swagger-api.json -g typescript-axios -o src/lib/openapi --additional-properties=supportsES6=true --skip-validate-spec "
  },
